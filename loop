public class App {
    public static void main(String[] args) throws Exception {
        
        for (int i = 5; i !=0; i--) {
            System.out.println ("Beatriz");
        }
        
        int nicknameCount = 5;
        
        while (nicknameCount!= 0) {
            System.out.println ("Beyaaaboo");
            nicknameCount--;
        }
        
         int surnameCount = 5;
        
        do{
            System.out.println ("Asuncion");
            surnameCount--;
        } while (surnameCount != 0);
        
    }
}
